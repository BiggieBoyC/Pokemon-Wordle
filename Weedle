def prints(ggen,pgen,gtype,ptype,gevo,pevo):
    if ggen==pgen:
        print ('Correct generation')
    if gtype==ptype:
        print ('Correct Type')
    if gevo==pevo:
        print ('Correct evolutionary Stage')
def main():
